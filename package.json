{
  "name": "11ty-theme-ui",
  "version": "1.0.0",
  "description": "11ty static with ThemeUI support",
  "scripts": {
    "dev": "eleventy --serve",
    "build": "yarn run clean && cross-env ELEVENTY_ENV=production eleventy",
    "watch": "eleventy --watch",
    "debug": "DEBUG=* eleventy",
    "clean": "rm -rf dist",
    "start": "yarn run clean && yarn run serve"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/deckchairlabs/11ty-theme-ui"
  },
  "author": "James Edmonds",
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@theme-ui/css": "^0.3.1",
    "@theme-ui/preset-base": "^0.3.0",
    "cross-env": "^7.0.2",
    "cssnano-preset-advanced": "^4.0.7",
    "htmlnano": "^0.2.5",
    "jsdom": "^16.2.2",
    "json5": "^2.2.2"
  },
  "devDependencies": {
    "create-emotion-server": "^10.0.27",
    "emotion": "^10.0.27"
  }
}
