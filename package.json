{
    "name": "lean",
    "version": "1.0.0",
    "description": "A static site boilerplate with ThemeUI support",
    "scripts": {
        "dev": "eleventy --serve",
        "build": "yarn run clean && cross-env ELEVENTY_ENV=production eleventy",
        "watch": "eleventy --watch",
        "debug": "DEBUG=* eleventy",
        "clean": "rm -rf dist",
        "start": "yarn run clean && yarn run serve"
    },
    "repository": {
        "type": "git",
        "url": "http://github.com/deckchairlabs/lean"
    },
    "author": "James Edmonds",
    "license": "MIT",
    "dependencies": {
        "@11ty/eleventy": "^0.11.0",
        "@theme-ui/css": "^0.3.1",
        "@theme-ui/preset-base": "^0.3.0",
        "clean-css": "^4.2.3",
        "cross-env": "^7.0.2",
        "dotenv": "^8.2.0",
        "eleventy-plugin-pwa": "^1.0.8",
        "htmlnano": "^0.2.5",
        "markdown-it": "^11.0.0",
        "markdown-it-anchor": "^5.3.0"
    },
    "devDependencies": {
        "create-emotion-server": "^10.0.27",
        "emotion": "^10.0.27"
    }
}
